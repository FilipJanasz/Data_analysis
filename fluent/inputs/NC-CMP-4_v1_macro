/solve/patch steam tube insulation_cone coolant () temperature 296.122
/define/operating-conditions/operating-pressure 2790.58
/solve/patch coolant () pressure 58329.0707
/solve/patch coolant () x-velocity n 0.063731
/file/write-case \"/home/janasz_f/NC-CMP-4_v1/NC-CMP-4_v1.cas\" ok
/file/auto-save/root-name \"/home/janasz_f/NC-CMP-4_v1/NC-CMP-4_v1.cas\"
	/solve/dual-time-iterate 1000 20 n ok n ok 
	/solve/set/time-step 1
	/solve/dual-time-iterate 3500 20 y y 
/plot/plot n \"/home/janasz_f/NC-CMP-4_v1/h2o_centerline.txt\" yes n n h2o y 1 0 symmetry ()
/plot/plot n \"/home/janasz_f/NC-CMP-4_v1/temp_centerline.txt\" yes n n temperature y 1 0 symmetry ()
