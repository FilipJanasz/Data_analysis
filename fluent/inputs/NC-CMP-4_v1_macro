/solve/monitors/volume/set-monitor heMass \"Volume Integral\" he steam () n n yes \"/home/janasz_f/NC-CMP-4_v1/heMass.out\" 1 n flow-time
(rp-var-define 'cttemp 393.4792 'real #f)
(rp-var-define 'steamtemp 416.5057 'real #f)
	/solve/initialize/initialize-flow
	/solve/initialize/compute-defaults/mass-flow-inlet steam_inlet
	/solve/initialize/initialize-flow ok
/solve/patch steam () species-0 0.038015
