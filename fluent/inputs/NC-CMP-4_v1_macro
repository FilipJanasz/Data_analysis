(cx-macro-define
 '( (runmacr . "
	/file/read-case \"/home/janasz_f/2017_2D_template/template30kHe.cas\" ok
/define/boundary-conditions/mass-flow-inlet steam_inlet y y n 8.4425e-05 y y \"udf\" \"inlet_temp::libudf_2D\" n 0 y n 1 n 0 n n y 5 10 n n 1 n 0 y 
/define/boundary-conditions/mass-flow-inlet coolant_inlet y y n 0.082664 n 393.4792 n 0 y n 1 n 0 n n y 5 10 y
	/solve/monitors/surface/clear-monitors
	/solve/monitors/volume/clear-monitors
/solve/monitors/surface/set-monitor udm4 \"Integral\" udm-4 steam_tube () n n yes \"/home/janasz_f/NC-CMP-4_v1/udm4.out\" 1 n flow-time
/solve/monitors/surface/set-monitor inletTemp \"Area-Weighted Average\" temperature steam_inlet () n n yes \"/home/janasz_f/NC-CMP-4_v1/inletTemp.out\" 1 n flow-time
