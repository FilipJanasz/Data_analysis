(rp-var-define 'cttemp 396.1415 'real #f)
(rp-var-define 'steamtemp 416.6071 'real #f)
	/solve/initialize/initialize-flow
	/solve/initialize/compute-defaults/mass-flow-inlet steam_inlet
	/solve/initialize/initialize-flow ok
/solve/patch steam () species-0 0.19566
/define/operating-conditions/operating-pressure 753.937
/solve/patch coolant () pressure 30437.5237
