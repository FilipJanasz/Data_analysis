(cx-macro-define
 '( (runmacr . "
	/file/read-case \"/home/janasz_f/2017_2D_template/template30k.cas\" ok
/define/boundary-conditions/mass-flow-inlet steam_inlet y y n 0.00013432 n 416.6071 n 0 y n 1 n 0 n n y 5 10 n n 1 y
