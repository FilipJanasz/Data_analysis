/solve/monitors/volume/set-monitor h2o \"Volume-Average\" h2o steam () n n yes \"/home/janasz_f/NC-MFR-ABS-4_4/h2o.out\" 1 n flow-time
(rp-var-define 'cttemp 391.3442 'real #f)
(rp-var-define 'steamtemp 413.3751 'real #f)
	/solve/initialize/initialize-flow
	/solve/initialize/compute-defaults/mass-flow-inlet steam_inlet
	/solve/initialize/initialize-flow ok
/solve/patch steam () species-0 0.057582
/define/operating-conditions/operating-pressure 4373.47
/solve/patch coolant () pressure 58449.6071
/solve/patch coolant () x-velocity n 0.063725
/file/write-case \"/home/janasz_f/NC-MFR-ABS-4_4/NC-MFR-ABS-4_4.cas\" ok
/file/auto-save/root-name \"/home/janasz_f/NC-MFR-ABS-4_4/NC-MFR-ABS-4_4.cas\"
	/solve/dual-time-iterate 1000 20 n ok n ok 
	/solve/set/time-step 1
	/solve/dual-time-iterate 3500 20 y y 
/plot/plot/plot n \"/home/janasz_f/NC-MFR-ABS-4_4/h2o_centerline.txt\" yes n n h2o y 1 0 symmetry ()
/plot/plot/plot n \"/home/janasz_f/NC-MFR-ABS-4_4/temp_centerline.txt\" yes n n temperature y 1 0 symmetry ()
