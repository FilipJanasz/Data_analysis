(cx-macro-define
 '( (runmacr . "
	/file/read-case \"/home/janasz_f/2017_2D_template/template30k.cas\" ok
/define/boundary-conditions/mass-flow-inlet steam_inlet y y n 2.4911e-05 n 273.15 n 0 y n 1 n 0 n n y 5 10 n n 1 y
/define/boundary-conditions/mass-flow-inlet coolant_inlet y y n NaN n 273.15 n 0 y n 1 n 0 n n y 5 10 y
	/solve/monitors/surface/clear-monitors
	/solve/monitors/volume/clear-monitors
/solve/monitors/surface/set-monitor udm4 \"Integral\" udm-4 steam_tube () n n yes \"/home/janasz_f/NC-MFR-ABS-5_4/udm4.out\" 1 n flow-time
/solve/monitors/volume/set-monitor press \"Volume-Average\" absolute-pressure steam () n n yes \"/home/janasz_f/NC-MFR-ABS-5_4/press.out\" 1 n flow-time
/solve/monitors/volume/set-monitor h2o \"Volume-Average\" h2o steam () n n yes \"/home/janasz_f/NC-MFR-ABS-5_4/h2o.out\" 1 n flow-time
(rp-var-define 'cttemp 273.15 'real #f)
(rp-var-define 'steamtemp 273.15 'real #f)
	/solve/initialize/initialize-flow
	/solve/initialize/compute-defaults/mass-flow-inlet steam_inlet
	/solve/initialize/initialize-flow ok
/solve/patch steam () species-0 0.0063781
/define/operating-conditions/operating-pressure 5749.63
/solve/patch coolant () pressure 57369.5976
/solve/patch coolant () x-velocity n 0.06373
/file/write-case \"/home/janasz_f/NC-MFR-ABS-5_4/NC-MFR-ABS-5_4.cas\" ok
/file/auto-save/root-name \"/home/janasz_f/NC-MFR-ABS-5_4/NC-MFR-ABS-5_4.cas\"
	/solve/dual-time-iterate 1000 20 n ok n ok 
	/solve/set/time-step 1
	/solve/dual-time-iterate 3500 20 y y 
/plot/plot/plot n \"/home/janasz_f/NC-MFR-ABS-5_4/h2o_centerline\" yes n n h2o y 1 0 symmetry ()
/plot/plot/plot n \"/home/janasz_f/NC-MFR-ABS-5_4/temp_centerline\" yes n n temperature y 1 0 symmetry ()
/plot/plot n \"/home/janasz_f/NC-MFR-ABS-5_4/temp_wall\" yes n n temperature y 1 0 steam_tube ()
/plot/plot n \"/home/janasz_f/NC-MFR-ABS-5_4/h2o_wall\" y n n h2o y 1 0 steam_tube ()
/plot/plot n \"/home/janasz_f/NC-MFR-ABS-5_4/udm4_wall\" y n n udm-4 y 1 0 steam_tube ()
	")
