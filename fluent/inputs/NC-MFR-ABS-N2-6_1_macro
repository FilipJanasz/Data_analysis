(rp-var-define 'cttemp 407.9619 'real #f)
(rp-var-define 'steamtemp 429.4874 'real #f)
	/solve/initialize/initialize-flow
	/solve/initialize/compute-defaults/mass-flow-inlet steam_inlet
	/solve/initialize/initialize-flow ok
/solve/patch steam () species-0 0.02082
/define/operating-conditions/operating-pressure 1861.86
/solve/patch coolant () pressure 69206.0842
/solve/patch coolant () x-velocity n 0.063733
/file/write-case \"/home/janasz_f/NC-MFR-ABS-N2-6_1/NC-MFR-ABS-N2-6_1.cas\" ok
/file/auto-save/root-name \"/home/janasz_f/NC-MFR-ABS-N2-6_1/NC-MFR-ABS-N2-6_1.cas\"
	/solve/dual-time-iterate 1000 20 n ok n ok 
	/solve/set/time-step 1
	/solve/dual-time-iterate 3500 20 y y 
/plot/plot/plot n \"/home/janasz_f/NC-MFR-ABS-N2-6_1/h2o_centerline\" yes n n h2o y 1 0 symmetry ()
/plot/plot/plot n \"/home/janasz_f/NC-MFR-ABS-N2-6_1/temp_centerline\" yes n n temperature y 1 0 symmetry ()
