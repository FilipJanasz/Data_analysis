(cx-macro-define
 '( (runmacr . "
	/file/read-case \"/home/janasz_f/2017_2D_template/template30k.cas\" ok
/define/boundary-conditions/mass-flow-inlet steam_inlet y y n 0.00011483 n 429.4874 n 0 y n 1 n 0 n n y 5 10 n n 1 y
/define/boundary-conditions/mass-flow-inlet coolant_inlet y y n 0.081593 n 407.9619 n 0 y n 1 n 0 n n y 5 10 y
	/solve/monitors/surface/clear-monitors
	/solve/monitors/volume/clear-monitors
/solve/monitors/surface/set-monitor udm4 \"Integral\" udm-4 steam_tube () n n yes \"/home/janasz_f/NC-MFR-ABS-N2-6_1/udm4.out\" 1 n flow-time
